<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EndlessRecyclerView">
        <attr name="scrollEndGravity" format="enum">
            <enum name="left" value="0"/>
            <enum name="top" value="1"/>
            <enum name="right" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
        <attr name="progressBarViewId" format="reference" />
        <attr name="swipeRefreshLayoutId" format="reference"/>
    </declare-styleable>
</resources>
